@model IEnumerable<Delives.pk.Models.ItemViewModel>
@if (Model != null && Model.Count() > 5)
{
    <div class="col-md-6 load-popular-items-left">
        @{Html.RenderPartial("~/Views/Delivery/_ListItemPartial.cshtml", Model.Take(3));}

    </div><!-- End col-md-6-->
    <div class="col-md-6 load-popular-items-right">
        @{Html.RenderPartial("~/Views/Delivery/_ListItemPartial.cshtml", Model.Skip(3).Take(3));}
    </div><!-- End col-md-6-->
}